server {
    listen 80 default_server;
    listen [::]:80 default_server;
    server_name 54.237.26.44;

    add_header X-Served-By 79001-web-01;

    location /airbnb-onepage/ {
        proxy_pass http://127.0.0.1:5000;
    }

    error_page 404 /404.html;
    location = /404.html {
        root /var/www/html;
        internal;
    }
}
